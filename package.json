{
  "name": "xacti",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "yarn create-help-message && yarn ts-node-dev -r dotenv/config src/index.ts",
    "test": "jest -c jest.config.js --coverage",
    "start": "node dist/src/index.js",
    "build": "tsc --project tsconfig.build.json && yarn create-help-message && yarn move-files",
    "heroku-postbuild": "echo \"xacti doesn't have the build process on heroku\"",
    "create-assets": "sh scripts/createAssets.sh",
    "move-files": "mv assets dist",
    "create-help-message": "yarn create-assets && ts-node-dev scripts/createHelpMessage.ts"
  },
  "dependencies": {
    "discord.js": "^12.5.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^14.14.16",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.5",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.3"
  }
}
